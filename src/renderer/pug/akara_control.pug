div(class="akara-control")
   div(class="akara-time")
      div(class="akara-time-length")
        div(class="akara-time-current")
            //-span(class="fa fa-circle akara-time-dragger")
   div(class="akara-control-element")
      span(class="akara-update-cur-time") 00:00 / 00:00
      ul(class="akara-media-control-left")
         li(class="fa fa-play" data-fire="play")
         li(class="fa fa-pause akara-display" data-fire="pause")
         li(class="fa fa-stop" data-fire="stop")
         li(class="fa fa-backward" data-fire="previous")
         li(class="fa fa-forward" data-fire="next")
      ul(class="akara-media-control-right")
         li(class="fa fa-cc" data-fire="subtitle")
         li(class="fa fa-repeat" data-fire="repeat")
         li(class="fa fa-asterisk" data-fire="loop")
